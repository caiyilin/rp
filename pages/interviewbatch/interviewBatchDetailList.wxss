page {
  background: #f8f8f8;
}

.search-container {
  background: #ededed;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
}

.search-bar {
  position: inherit !important;
  background: transparent !important;
}

.search-bar .wux-search-bar__form {
  border-radius: 16rpx !important;
}

.search-bar .wux-search-bar__label {
  position: static !important;
}

.list-content {
  clear: both;
  padding-top: 92rpx;
}
