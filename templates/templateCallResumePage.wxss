@import "/templates/templateCallResumeList.wxss";
@import "/templates/templateNoData.wxss";

.search-container {
  background: #ededed;
  display: flex;
  width: 100%;
  /* position: fixed; */
  /* top: 0; */
  /* left: 0; */
  z-index: 999;
}

.search-bar {
  position: inherit !important;
  background: transparent !important;
}

.search-bar .wux-search-bar__form {
  border-radius: 16rpx !important;
}

.search-bar .wux-search-bar__label {
  position: static !important;
}

.wux-dialog__buttons {
  padding: 0rpx 20rpx !important;
  margin-bottom: 30rpx;
}

.wux-dialog__button {
  border-radius: 10rpx !important;
  margin-bottom: 20rpx !important;
  border: 1px solid #c1c1c1;
  color: #acacac !important;
}

.wux-dialog__button:first-child {
  background: #e3f4e4 !important;
  border: 1px solid #7fcf89;
}

.wux-popup__hd {
  line-height: 100rpx;
}

.wux-popup__bd {
  display: none;
}

.wux-popup__ft {
  position: static !important;
}

.search-left {
  float: left;
  width: 90%;
}

.search-right {
  position: relative;
  float: right;
  right: 10rpx;
}

.filter-bar .wux-filterbar__bd {
  background: transparent !important;
  height: 85rpx;
}

.filter-bar .wux-filterbar__bd:after {
  border-bottom: 0;
}

.filter-bar .wux-filterbar__panel-selected {
  line-height: 42rpx;
  height: 42rpx;
}

.list-content {
  clear: both;
  /* padding-top: 92rpx; */
}

.fab-round {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50rpx;
  position: fixed;
  background: #fff;
  border: 1px solid #e2e2e2;
  line-height: 50rpx;
  right: 20rpx;
  bottom: 20rpx;
  color: #333;
  font-size: 20rpx;
  text-align: center;
  z-index: 999;
}

.round-up {
  width: 70rpx;
  position: fixed;
  right: 18rpx;
  bottom: 40rpx;
}

.round-up-text {
  position: fixed;
  bottom: 20rpx;
  right: 34rpx;
}